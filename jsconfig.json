{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["dom"],
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
