{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["es7", "dom"],
    "typeRoots": ["node_modules/@types"],
    "outDir": "dist123",
    "sourceMap": true,
    "removeComments": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@@/*": ["/src/*"]
    }
  },
  "exclude": ["node_modules"]
}
